-- some-what open source
Width = 40
Height =35

LP = 17
RP = 17

function Output1(Output1)
	local args = {
		[1] = "Update",
		[2] = {
			["DescriptionText"] = Output1,
			["ImageId"] = 11811676086
		}
	}

	game:GetService("ReplicatedStorage").CustomiseBooth:FireServer(unpack(args))
end

